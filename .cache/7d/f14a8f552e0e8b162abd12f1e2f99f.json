\n\n$side-margin: 5rem;\n\n$linkBB: $near-black 2px solid;","../sass/components/_nav.scss":".nav {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  z-index: 9999;\n  background-color: $white;\n\n  &-top {\n    margin: 2rem $side-margin;\n    @include betweenFlex;\n    flex-flow: wrap;\n    margin-bottom: 2.5rem;\n\n    &__logo {\n      @include centerFlex;\n      @include linkTrans;\n    }\n\n    &__search {\n      font-size: 2rem;\n      border: $black 1px solid;\n      border-radius: 0;\n      outline: none;\n      min-width: 25%;\n    }\n\n    &__account {\n      @include centerFlex;\n\n      div {\n        margin-right: 2.5rem;\n      }\n    }\n  }\n\n  &-bot {\n    font-size: 1.5rem;\n    border-top: #ccc 1px solid;\n    border-bottom: #ccc 1px solid;\n    margin-bottom: 2rem;\n\n    &__container {\n      @include betweenFlex;\n      flex-flow: wrap;\n      margin: .5rem $side-margin;\n\n    }\n\n    &__link {\n      border-bottom: transparent 1px solid;\n      transition: border .25s;\n\n      &:active,\n      &:hover {\n        border-bottom: $near-black 1px solid;\n      }\n    }\n  }\n\n}","../sass/abstracts/_mixins.scss":"@mixin centerFlex {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n@mixin betweenFlex {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n@mixin fixedCenter {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n@mixin centerGrid {\n  display: grid;\n  justify-content: center;\n  align-items: center;\n}\n\n@mixin verticalPageMargin {\n  margin-top: 20rem;\n  margin-bottom: 5rem;\n}\n\n@mixin boxShadow {\n  box-shadow: 1rem 1rem 5rem rgba($black, 0.5);\n}\n\n@mixin boxShadow-thick {\n  box-shadow: 1rem 1rem 5rem rgba($black, 0.75);\n}\n\n@mixin fixedBG {\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-attachment: fixed;\n  background-position: center;\n}\n\n@mixin linkTrans {\n  div {\n    border-bottom: transparent 2px solid;\n    transition: all .2s;\n\n    &:hover,\n    &:active {\n      color: $near-black;\n      border-bottom: $linkBB;\n    }\n  }\n}","../sass/components/_footer.scss":".footer {\n  padding: 2rem;\n  background-color: $dark-main;\n\n  &__container {\n    display: flex;\n    justify-content: space-evenly;\n    align-items: center;\n  }\n\n  &__logo {\n    font-size: 5rem;\n    @include centerFlex;\n    @include linkTrans;\n  }\n\n  &__hero {\n    @include centerFlex;\n  }\n\n  &__column {\n    margin: 2rem 2rem;\n  }\n}","../sass/pages/_home.scss":".home {\n  @include centerFlex;\n  @include verticalPageMargin;\n\n  &__photo {\n    height: 40rem;\n\n    &:first-child {\n      margin-right: 1rem;\n    }\n  }\n}","../sass/pages/_product.scss":".product {\n  margin-top: 20rem;\n  height: 75vh;\n}"},"lineCount":null}},"error":null,"hash":"3d64df6873b3e13ee0c6ceb0f188d183","cacheData":{"env":{}}}